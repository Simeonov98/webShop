{% extends 'base.html.twig' %}
{% block body %}


        <div class="container">
            <div class="row justify-content-md-center">
                <div class="col col-lg-5">
                    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" style="height: 395px">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="{{ asset(latestArticles3.imageId) }}" class="d-block w-100"
                                     alt="latestArticle1">
                            </div>
                            <div class="carousel-item">
                                <img src="{{ asset(latestArticles2.imageId) }}" class="d-block w-100"
                                     alt="latestArticle2">
                            </div>
                            <div class="carousel-item">
                                <img src="{{ asset(latestArticles1.imageId) }}" class="d-block w-100"
                                     alt="latestArticle3">
                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleControls" role="button"
                           data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleControls" role="button"
                           data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
{% endblock %}